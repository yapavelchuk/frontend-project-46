{
  "name": "@hexlet/code",
  "version": "1.0.0",
  "type": "module",
  "description": "[![Actions Status](https://github.com/yapavelchuk/frontend-project-46/workflows/hexlet-check/badge.svg)](https://github.com/yapavelchuk/frontend-project-46/actions)",
  "main": "index.js",
  "bin": {
    "gendiff": "bin/gendiff.js"
  },
  "scripts": {
    "lint": "eslint . --fix && stylelint **/*.scss && htmlhint **/*.html",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "codeclimate": "npm run test:coverage && codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yapavelchuk/frontend-project-46.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yapavelchuk/frontend-project-46/issues"
  },
  "homepage": "https://github.com/yapavelchuk/frontend-project-46#readme",
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "commander": "^10.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "babel-jest": "^29.5.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
  }
}
}
